{
  "name": "cloudwatch-front-logger",
  "version": "0.0.10",
  "description": "Save your browser console logs to AWS CloudWatch (Inspired by agea/console-cloud-watch)",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "typings/index.d.ts",
  "keywords": [
    "aws",
    "cloudwatch",
    "logging",
    "browser"
  ],
  "dependencies": {
    "@aws-sdk/client-cloudwatch-logs": "^3.10.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/plugin-transform-regenerator": "^7.7.5",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-typescript": "^7.14.5",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@types/jest": "^26.0.23",
    "@types/node": "^12.12.14",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "jest": "^27.0.6",
    "jest-date-mock": "^1.0.8",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "rollup": "^2.52.6",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.5"
  },
  "scripts": {
    "check": "tsc --noEmit",
    "lint:eslint": "eslint {src,test}/**/*.ts",
    "lint:prettier": "prettier ./src ./tests --list-different",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "fix:eslint": "eslint {src,tests}/**/*.ts",
    "fix:prettier": "prettier ./src ./tests --write",
    "fix": "npm run fix:eslint && npm run fix:prettier",
    "build:cjs": "BABEL_ENV=cjs babel src --out-dir lib --extensions .ts",
    "build:es": "BABEL_ENV=es babel src --out-dir es --extensions .ts",
    "build:umd": "BABEL_ENV=es NODE_ENV=development rollup -c",
    "build:umd:min": "BABEL_ENV=es NODE_ENV=production rollup -c",
    "build:types": "tsc --outDir typings/ --emitDeclarationOnly",
    "build": "run-p build:**",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "git diff --no-ext-diff --quiet --exit-code src .gitignore package.json README.md LICENSE || (echo \"Please commit changes!\" && false) && npm test && rm -rf lib es dist typings && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mpyw/cloudwatch-front-logger.git"
  },
  "author": "mpyw",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mpyw/cloudwatch-front-logger/issues"
  },
  "homepage": "https://github.com/mpyw/cloudwatch-front-logger#readme"
}
